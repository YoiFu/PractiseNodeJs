<div class="studentView">
  <div class="row">
      {{#each result}}
        <div class="card" style="width: 20rem;">
          <div class="card-body">
            <h5 class="card-title">{{this.name}}</h5>
            <h6 class="card-subtitle mb-2 text-body-secondary">"Age: "{{this.age}}</h6>
            <p class="card-text">"Major: "{{this.major}}</p>
            <a href="#" class="btn btn-primary">See Profile</a>
          </div>
        </div>
      {{/each}}
  </div>
</div>